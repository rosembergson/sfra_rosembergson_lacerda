{
  "name": "training",
  "version": "1.0.0",
  "description": "Training",
  "engines": {
    "node": ">=8.11.3"
  },
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "sgmf-scripts --cover",
    "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
